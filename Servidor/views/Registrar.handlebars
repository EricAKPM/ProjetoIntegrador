<form action="/RegistrarEnviar" method="POST">
    <input type="text" name="Nome" value="{{Nome}}"></input>
    <input type="text" name="Senha" value="{{Senha}}"></input>
    <input type="text" name="Email" value="{{Email}}"></input>
    <input type="text" name="DataNascimento" value="{{DataNascimento}}"></input>
    <button type="submit">ENVIAR</button>
</form>